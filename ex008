#include<stdio.h>
#include<stdlib.h>

int main()
{
 int mtz [3][3], mtzz[3][3], i, j;

 for(i=0; i<3; i++){
    for(j=0; j<3; j++){
        printf("Insira um numero | %d| |%d|: ", i, j);
        scanf("%d", &mtz[i][j]);
        mtzz[i][j] = mtz[i][j] * 2;
    }
 }
 printf("\n");
 for(i=0; i<3; i++){
    for(j=0; j<3; j++){
        printf("\n|%d|", mtz[i][j]);
    }
    printf("\n");
 }

  printf("Resultado multiplicado por 2\n");
 for(i=0; i<3; i++){
    for(j=0; j<3; j++){
        printf("\n|%d|", mtzz[i][j]);
    }
    printf("\n");
 }

 return 0;
}
