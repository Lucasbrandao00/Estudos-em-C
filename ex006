#include<stdio.h>

	int main ()
	{
		int vet[20], i=0, cont=0, soma=0;

			do{
				printf("Digite um numero: ");
				scanf("%d", &vet[i]);
				i++;
			}while(i<20);

		for(i=0;i<20;i++){
			if(vet[i]>10){
				soma+=vet[i];
				cont++;
			}
		}

		for(i=0;i<20;i++){
			printf("%d\n", vet[i]);
		}

		printf("Sao %d numeros maiores que 10!\n", cont);
		printf("A soma dos numeros maiores que 10 e: %d", soma);
	return 0;
	}
